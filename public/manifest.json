{
  "manifest_version": 2,
  "name": "Yafix",
  "description": "Fixes that annoying pause bug",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://music.yandex.ru/*"],
      "js": ["inject.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
     "script.js"
  ]
}
